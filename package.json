{
  "name": "jp-dict",
  "version": "1.1.0",
  "workspaces": [
    ".",
    "front",
    "back"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "cd front && ng build --configuration production",
    "package": "npm run build && electron-packager . jp-dict --platform=win32 --arch=x64 --out=dist --overwrite"
  },
  "dependencies": {
    "electron": "^25.2.0"
  },
  "devDependencies": {
    "electron-packager": "^15.4.0",
    "concurrently": "^7.6.0",
    "lerna": "^8.1.3",
    "npm-version": "^1.1.0",
    "wait-on": "^7.0.1"
  }
}
